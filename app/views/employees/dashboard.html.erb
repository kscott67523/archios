<div class="container-fluid">
  <div class="row">
    <div class="col-md 12">
      <h1>Your Employees</h2>
    </div>
  </div>
  <div>
    <%#indent if off%>
    <hr>

    <div class="row">
      <div class="col-md 12">
        <% if current_employee.manager? %>
          <%= render "employees", employee: current_employee %>
        <% end %>
      </div>
    </div>
  </div>
</div>
